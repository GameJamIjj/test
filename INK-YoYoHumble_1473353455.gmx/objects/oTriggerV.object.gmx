        <arguments>
          <argument>
            <kind>1</kind>
            <string>var vxNew, vyNew;

// Handle sub-pixel movement
cx += vx;
cy += vy;
vxNew = round(cx);
vyNew = round(cy);
cx -= vxNew;
cy -= vyNew;

repeat (abs(vyNew)) {
    if (!place_meeting(x, y + (sign(vyNew)), oStopper)) {
        with (oParEntity) {
            if (place_meeting(x, y + 1, other.id)) {
                if (!PlaceMeetingException(x, y + sign(vyNew), oParSolid, other.id))
                    y += sign(vyNew);
            }
            
            if (place_meeting(x, y - 1, other.id))
                y += sign(vyNew);
            
        }
        
        with (oParEnemy) {
            if (place_meeting(x, y + 1, other.id)) {
                if (!PlaceMeetingException(x, y + sign(vyNew), oParSolid, other.id))
                    y += sign(vyNew);
            }
            
            if (place_meeting(x, y - 1, other.id))
                y += sign(vyNew);
            
        }
        
        y += sign(vyNew);
    } else {
        //move = false;
        vy = 0;
        if (!hasPlayedSound)
        {
            audio_play_sound(sfxCrusherFall, 0, 0);
            hasPlayedSound = true;
        }
        break;
    }
}
</string>
